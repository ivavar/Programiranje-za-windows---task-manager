        <TextBox Height="27" HorizontalAlignment="Left" Margin="108,27,0,0" Name="textBox1" VerticalAlignment="Top" Width="337" Text="Type the name of a program, folder, document, or Internet " BorderThickness="0" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="108,45,0,0" Name="textBox2" VerticalAlignment="Top" Width="337" Text="resource and Windows will open it for you." BorderThickness="0" />
